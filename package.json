{
  "name": "@jellycat-js/jellycat",
  "version": "0.1.26",
  "description": "Jellycat is a lightweight js framework for creating web components",
  "repository": "git://github.com/jellycat-js/jellycat.git",
  "bugs": "https://github.com/jellycat-js/jellycat/issues",
  "author": "Louis-Antoine Lumet (cxxi)",
  "license": "MIT",
  "main": "dist/bundle.cjs.js",
  "type": "module",
  "module": "dist/bundle.esm.js",
  "exports": {
    "import": "./dist/bundle.esm.js",
    "require": "./dist/bundle.cjs.js"
  },
  "files": [
    "src",
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "keywords": [
    "jellycat",
    "framework",
    "light",
    "webcomponent"
  ],
  "node": ">=16.0.0",
  "engines": {
    "node": ">=16.x",
    "npm": ">=6.x"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@babel/preset-env": "^7.18.10",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-terser": "^0.2.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.2.4",
    "babel-jest": "^29.3.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "rollup-plugin-html": "^0.2.1"
  }
}
